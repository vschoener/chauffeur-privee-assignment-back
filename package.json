{
  "name": "chauffeur-privee",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run serve",
    "flow": "flow",
    "build": "npm run build:flow && npm run lint && npm run build:transpile",
    "build:transpile": "babel src -d dist",
    "watch": "concurrently -k -p '[{name}]' -n 'Build,Node' -c 'green.bold,blue.bold' 'npm run build:transpile -- --watch' 'nodemon dist/main.js'",
    "serve": "node dist/main.js",
    "test": "jest --forceExit",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.48.0",
    "eslint-plugin-import": "^2.12.0",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
