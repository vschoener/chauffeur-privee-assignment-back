    "express": "^4.16.3",
    "http-status-codes": "^1.3.0",
    "md5": "^2.2.1",
    "mongodb": "^3.0.10",
    "mongoose": "^5.1.3",
    "mongoose-long": "^0.2.1",
    "morgan": "^1.9.0",
    "winston": "^2.4.2"
  }
